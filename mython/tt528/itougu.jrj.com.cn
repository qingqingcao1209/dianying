            //重构数据
            function reorder (array, limit) {
                var _array = [].concat(array),
                    currentIndex = _array.length,
                    length = _array.length,
                    temporaryValue, randomIndex;

                while (0 !== currentIndex) {
                    randomIndex = Math.floor(Math.random() * currentIndex);
                    currentIndex -= 1;

                    temporaryValue = _array[currentIndex];
                    _array[currentIndex] = _array[randomIndex];
                    _array[randomIndex] = temporaryValue;
                }

                if( length > limit && (length % limit) ) {
                    var diff = limit - (length % limit), i=0;
                    while(i<diff){ _array.push(_array[i]); i+=1 }
                }

                return _array;
            }

