                ellipsis: function( str, limit ){
                    var w = 0, len = str.length;

                    for (var i=0; i < len; i++) {
                        var c = str.charCodeAt(i);
                        if ((c >= 0x0001 && c <= 0x007e) || (0xff60<=c && c<=0xff9f)) w++;
                        else w+=2;

                        if (w > limit) {
                            str = str.substr(0,i) + "...";
                            break;
                        }
                    }
                    
                    return str;
                },

                getInt: function( num ){
                    return parseInt( num );
                },

                getFloat: function( num ){
                    var _num = '' + num.toFixed(2);
                    return _num.split('.')[1];
                },

                showItem: function( elem ){
                    $(elem).filter('li').fadeIn();
                },

                switchupAside: function(data, evt) { //侧边栏滚动
                    var listWrapper = $(evt.target).parents('.switchup').next(),
                        listWrapperHeight = listWrapper.height(),
                        list = listWrapper.find('ul'), 
                        item = list.find('li'),
                        itemHeight = item.outerHeight(),
                        visibleItem = Math.round(listWrapperHeight / itemHeight);

                    list.animate({
                        top: -visibleItem * itemHeight
                    }, 500, function(){
                        var i = 0;
                        while( i < visibleItem ) {
                            list.find('li').eq(0).appendTo(list); i+=1;
                        }
                        list.css('top', '');
                        $(".aside ul li img").unveil(0, function () {
                            this.style.opacity = 1;
                        });
                    });
                },

                switchupMain: function(data, evt){
                    var list = $(evt.target).parents('p').prev(),
                        num = list.find('li').length,
                        type = list.data('type'),
                        dataArr = window[type],
                        _dataArr = DATA_LIST[type];

                    if( list.data('counter')+1 == Math.ceil(dataArr.length / num) ) {
                        list.data('counter', 0);
                        _dataArr = reorder( dataArr, num );
                    } 
                    else list.data('counter', list.data('counter') + 1);

                    var counter = list.data('counter');
                    this[type]( _dataArr.slice(counter * num, (counter+1) * num) );
                    F.hoverCls( '.headImageClass1' , 'aitgid' );
                    $(".datalist img").unveil(0, function () {
                        this.style.opacity = 1;
                    });
                }
            });

            $('.section.main .datalist ul').removeClass('loading');

            $('.switchable-content').switchable({
                effect: "fade",
                panels: 'li',
                interval: 2,
                autoplay: true,
                onSwitch: function(evt, index){
                    $('.switchable-bg li.curr').fadeOut().removeClass('curr');
                    $('.switchable-bg li').eq(index).fadeIn().addClass('curr');
                }
            });
            
